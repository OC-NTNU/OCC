        <infon key="n">1</infon>
        <offset>0</offset>
        <text>Eddy-induced reduction of biological production in eastern boundary upwelling systems
</text>
        <annotation id="T1">
          <infon key="brat-id">T1</infon>
          <infon key="type">Variable</infon>
          <infon key="brat-type">text-bound</infon>
          <location length="59" offset="26"/>

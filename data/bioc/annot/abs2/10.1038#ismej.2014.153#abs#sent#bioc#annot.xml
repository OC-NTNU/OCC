          <infon key="brat-id">T1</infon>
          <infon key="type">Variable</infon>
          <infon key="brat-type">text-bound</infon>
          <location length="80" offset="40"/>
          <text>the marine bacterioplankton community throughout the water column over ten years</text>
        </annotation>
        <relation id="E1">
          <infon key="brat-id">E1</infon>

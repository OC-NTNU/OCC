          <infon key="brat-type">text-bound</infon>
          <location length="80" offset="985"/>
          <text>he cooling effect owing to carbon dioxide fertilization of the terrestrial biota</text>
        </annotation>
        <relation id="E8">
          <infon key="brat-id">E8</infon>
          <infon key="type">Decrease</infon>
          <infon key="brat-type">event</infon>

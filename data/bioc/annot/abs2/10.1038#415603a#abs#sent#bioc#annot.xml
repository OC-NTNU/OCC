      <sentence>
        <infon key="n">3</infon>
        <offset>213</offset>
        <text>The physical mechanisms responsible for these fluctuations are poorly understood.
</text>
        <annotation id="T7">
          <infon key="brat-id">T7</infon>
          <infon key="type">RefExp</infon>
          <infon key="brat-type">text-bound</infon>

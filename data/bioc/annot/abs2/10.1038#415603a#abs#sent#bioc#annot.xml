        </annotation>
        <annotation id="T10">
          <infon key="brat-id">T10</infon>
          <infon key="type">Decrease</infon>
          <infon key="brat-type">text-bound</infon>
          <location length="7" offset="530"/>
          <text>slowing</text>
        </annotation>
        <annotation id="T11">
          <infon key="brat-id">T11</infon>

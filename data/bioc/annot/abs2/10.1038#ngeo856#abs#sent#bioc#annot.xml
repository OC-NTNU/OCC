        </annotation>
        <annotation id="T10">
          <infon key="brat-id">T10</infon>
          <infon key="type">Increase</infon>
          <infon key="brat-type">text-bound</infon>
          <location length="8" offset="1172"/>
          <text>enhanced</text>
        </annotation>
        <relation id="E10">
          <infon key="brat-id">E10</infon>

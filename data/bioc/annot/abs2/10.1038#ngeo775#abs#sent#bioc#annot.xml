          <text>lower</text>
        </annotation>
        <annotation id="T17">
          <infon key="brat-id">T17</infon>
          <infon key="type">Variable</infon>
          <infon key="brat-type">text-bound</infon>
          <location length="32" offset="852"/>
          <text>normal nannoplankton populations</text>
        </annotation>
        <annotation id="T18">
          <infon key="brat-id">T18</infon>
          <infon key="type">Increase</infon>
          <infon key="brat-type">text-bound</infon>
          <location length="8" offset="840"/>
          <text>recovery</text>
        </annotation>
        <relation id="E8">
          <infon key="brat-id">E8</infon>

        <relation id="E2">
          <infon key="brat-id">E2</infon>
          <infon key="type">Decrease</infon>
          <infon key="brat-type">event</infon>
          <node refid="T3" role="Decrease"/>
          <node refid="T5" role="Theme"/>
        </relation>
      </sentence>
      <sentence>
        <infon key="n">4</infon>
        <offset>402</offset>
        <text>A host of climate-sensitive factors can affect this balance, including temperature, oxygen concentration, stratification, community composition and the mineral content of the sinking particles.
</text>
        <annotation id="T4">
          <infon key="brat-id">T4</infon>
          <infon key="type">Variable</infon>
          <infon key="brat-type">text-bound</infon>
          <location length="9" offset="585"/>
          <text>particles</text>
        </annotation>
        <annotation id="T7">
          <infon key="brat-id">T7</infon>
          <infon key="type">Change</infon>
          <infon key="brat-type">text-bound</infon>
          <location length="7" offset="577"/>
          <text>sinking</text>
        </annotation>
        <relation id="E4">
          <infon key="brat-id">E4</infon>

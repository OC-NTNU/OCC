      <sentence>
        <infon key="n">6</infon>
        <offset>780</offset>
        <text>We used molecular ecologic methods to study changes in bacterial community structure in response to the oxygenation in the Byfjord.
</text>
        <annotation id="T11">
          <infon key="brat-id">T11</infon>
          <infon key="type">Variable</infon>
          <infon key="brat-type">text-bound</infon>
          <location length="29" offset="835"/>
          <text>bacterial community structure</text>
        </annotation>
        <annotation id="T12">
          <infon key="brat-id">T12</infon>
          <infon key="type">Change</infon>
          <infon key="brat-type">text-bound</infon>
          <location length="7" offset="824"/>
          <text>changes</text>
        </annotation>
        <annotation id="T13">
          <infon key="brat-id">T13</infon>
          <infon key="type">I_Change</infon>
          <infon key="brat-type">text-bound</infon>
          <location length="30" offset="880"/>

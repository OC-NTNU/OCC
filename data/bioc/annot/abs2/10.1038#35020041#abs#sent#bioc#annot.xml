          <infon key="brat-type">text-bound</infon>
          <location length="4" offset="928"/>
          <text>iron</text>
        </annotation>
        <annotation id="T15">
          <infon key="brat-id">T15</infon>
          <infon key="type">Variable</infon>
          <infon key="brat-type">text-bound</infon>
          <location length="38" offset="959"/>
          <text>upwelling rate of nutrient-rich waters</text>
        </annotation>
        <annotation id="T16">
          <infon key="brat-id">T16</infon>
          <infon key="type">Change</infon>
          <infon key="brat-type">text-bound</infon>

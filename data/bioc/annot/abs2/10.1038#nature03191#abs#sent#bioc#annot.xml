        <infon key="n">2</infon>
        <offset>78</offset>
        <text>Oceanic dissolved organic carbon (DOC) constitutes one of the largest pools of reduced carbon in the biosphere.
</text>
        <annotation id="T1">
          <infon key="brat-id">T1</infon>
          <infon key="type">Variable</infon>
          <infon key="brat-type">text-bound</infon>
          <location length="23" offset="165"/>
          <text>carbon in the biosphere</text>
        </annotation>
        <annotation id="T2">

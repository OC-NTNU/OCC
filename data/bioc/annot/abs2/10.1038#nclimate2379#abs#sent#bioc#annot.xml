      <infon key="type">body</infon>
      <sentence>
        <infon key="n">2</infon>
        <offset>86</offset>
        <text>Although ocean warming and acidification are recognized as two major anthropogenic perturbations of today's oceans we know very little about how marine phytoplankton may respond via evolutionary change.
</text>
        <annotation id="T1">
          <infon key="brat-id">T1</infon>
          <infon key="type">Variable</infon>
          <infon key="brat-type">text-bound</infon>
          <location length="20" offset="231"/>
          <text>marine phytoplankton</text>
        </annotation>
        <annotation id="T2">
          <infon key="brat-id">T2</infon>
          <infon key="type">Change</infon>
          <infon key="brat-type">text-bound</infon>
          <location length="6" offset="281"/>
          <text>change</text>
        </annotation>
        <relation id="E1">
          <infon key="brat-id">E1</infon>

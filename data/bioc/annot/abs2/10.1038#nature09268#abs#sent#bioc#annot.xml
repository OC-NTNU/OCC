          <node refid="T6" role="Decrease"/>
          <node refid="T5" role="Theme"/>
        </relation>
      </sentence>
      <sentence>
        <infon key="n">6</infon>
        <offset>750</offset>
        <text>Our analyses further reveal interannual to decadal phytoplankton fluctuations superimposed on long-term trends.
</text>
        <annotation id="T7">
          <infon key="brat-id">T7</infon>
          <infon key="type">Variable</infon>
          <infon key="brat-type">text-bound</infon>
          <location length="36" offset="778"/>
          <text>interannual to decadal phytoplankton</text>
        </annotation>
        <annotation id="T8">
          <infon key="brat-id">T8</infon>
          <infon key="type">Change</infon>
          <infon key="brat-type">text-bound</infon>
          <location length="12" offset="815"/>
          <text>fluctuations</text>
        </annotation>
        <annotation id="T9">
          <infon key="brat-id">T9</infon>

          <infon key="brat-type">event</infon>
          <node refid="T13" role="Decrease"/>
          <node refid="T14" role="Theme"/>
        </relation>
      </sentence>
      <sentence>
        <infon key="n">4</infon>
        <offset>346</offset>
        <text>Picophytoplankton, major primary producers in the ocean, have until recently been thought unimportant regarding particle transport.
</text>
        <annotation id="T15">
          <infon key="brat-id">T15</infon>
          <infon key="type">Variable</infon>
          <infon key="brat-type">text-bound</infon>

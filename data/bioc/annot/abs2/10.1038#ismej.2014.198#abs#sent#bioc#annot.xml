      <sentence>
        <infon key="n">5</infon>
        <offset>735</offset>
        <text>Metagenomic coverage was not correlated with transcriptional activity for several key taxa within Bacteria.
</text>
        <annotation id="T26">
          <infon key="brat-id">T26</infon>
          <infon key="type">Decrease</infon>
          <infon key="brat-type">text-bound</infon>

          <infon key="brat-type">text-bound</infon>
          <location length="3" offset="728"/>
          <text>low</text>
        </annotation>
        <annotation id="T5">
          <infon key="brat-id">T5</infon>
          <infon key="type">Variable</infon>
          <infon key="brat-type">text-bound</infon>
          <location length="11" offset="733"/>

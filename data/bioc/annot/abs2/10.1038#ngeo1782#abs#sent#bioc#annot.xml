      <sentence>
        <infon key="n">5</infon>
        <offset>658</offset>
        <text>Changes in nutrient utilization should be reflected in the delta13C gradient between intermediate and deep waters.
</text>
        <annotation id="T21">
          <infon key="brat-id">T21</infon>
          <infon key="type">Variable</infon>
          <infon key="brat-type">text-bound</infon>

        </annotation>
        <annotation id="T23">
          <infon key="brat-id">T23</infon>
          <infon key="type">I_Change</infon>
          <infon key="brat-type">text-bound</infon>
          <location length="58" offset="713"/>
          <text>the delta13C gradient between intermediate and deep waters</text>
        </annotation>
        <annotation id="T24">

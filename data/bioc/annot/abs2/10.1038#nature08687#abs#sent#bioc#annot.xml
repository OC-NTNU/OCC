          <text>correlated</text>
        </annotation>
        <annotation id="T17">
          <infon key="brat-id">T17</infon>
          <infon key="type">Variable</infon>
          <infon key="brat-type">text-bound</infon>
          <location length="25" offset="838"/>
          <text>Zonally integrated carbon</text>
        </annotation>
        <relation id="E8">
          <infon key="brat-id">E8</infon>

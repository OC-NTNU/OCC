          <infon key="brat-type">text-bound</infon>
          <location length="20" offset="908"/>
          <text>Sedimentary delta15N</text>
        </annotation>
        <relation id="E11">
          <infon key="brat-id">E11</infon>
          <infon key="type">Change</infon>
          <infon key="brat-type">event</infon>

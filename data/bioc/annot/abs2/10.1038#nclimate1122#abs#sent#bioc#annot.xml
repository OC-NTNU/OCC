        </annotation>
        <annotation id="T19">
          <infon key="brat-id">T19</infon>
          <infon key="type">Variable</infon>
          <infon key="brat-type">text-bound</infon>
          <location length="2" offset="821"/>
          <text>pH</text>
        </annotation>
        <annotation id="T20">
          <infon key="brat-id">T20</infon>
          <infon key="type">Variable</infon>
          <infon key="brat-type">text-bound</infon>
          <location length="15" offset="851"/>

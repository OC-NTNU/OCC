          <text>upper-ocean</text>
        </annotation>
        <annotation id="T4">
          <infon key="brat-id">T4</infon>
          <infon key="type">Change</infon>
          <infon key="brat-type">text-bound</infon>
          <location length="14" offset="278"/>
          <text>stratification</text>
        </annotation>
        <relation id="E2">
          <infon key="brat-id">E2</infon>

          <infon key="brat-id">T5</infon>
          <infon key="type">I_Change</infon>
          <infon key="brat-type">text-bound</infon>
          <location length="48" offset="826"/>
          <text>the programmed cell death (PCD) of Trichodesmium</text>
        </annotation>
        <annotation id="T6">
          <infon key="brat-id">T6</infon>
          <infon key="type">Variable</infon>

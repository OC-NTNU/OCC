          <text>the neurotoxin domoic acid</text>
        </annotation>
        <annotation id="T10">
          <infon key="brat-id">T10</infon>
          <infon key="type">Increase</infon>
          <infon key="brat-type">text-bound</infon>
          <location length="7" offset="297"/>
          <text>produce</text>
        </annotation>
        <relation id="E6">
          <infon key="brat-id">E6</infon>
          <infon key="type">Increase</infon>
          <infon key="brat-type">event</infon>
          <node refid="T10" role="Increase"/>
          <node refid="T9" role="Theme"/>
        </relation>
      </sentence>
      <sentence>
        <infon key="n">4</infon>
        <offset>333</offset>
        <text>Recent studies suggest that algal blooms dominated by Pseudo-nitzschia are increasing in frequency and duration owing to changes in coastal nutrient regimes.
</text>
        <annotation id="T11">
          <infon key="brat-id">T11</infon>
          <infon key="type">Variable</infon>
          <infon key="brat-type">text-bound</infon>

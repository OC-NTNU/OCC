 @article{Riebesell_2007, title={Enhanced biological carbon consumption in a high CO2 ocean}, volume={450}, ISSN={1476-4687}, url={http://dx.doi.org/10.1038/nature06267}, DOI={10.1038/nature06267}, number={7169}, journal={Nature}, publisher={Nature Publishing Group}, author={Riebesell, U. and Schulz, K. G. and Bellerby, R. G. J. and Botros, M. and Fritsche, P. and Meyerhöfer, M. and Neill, C. and Nondal, G. and Oschlies, A. and Wohlers, J. and et al.}, year={2007}, month={Nov}, pages={545–548}}

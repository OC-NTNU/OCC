          <infon key="brat-type">text-bound</infon>
          <infon key="brat-id">T21</infon>
          <location length="48" offset="1093"/>
          <text>The stoichiometry of carbon to nitrogen drawdown</text>
        </annotation>
        <relation id="E8">
          <infon key="type">Decrease</infon>

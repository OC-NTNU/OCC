          <text>increased</text>
        </annotation>
        <annotation id="T18">
          <infon key="type">Variable</infon>
          <infon key="brat-type">text-bound</infon>
          <infon key="brat-id">T18</infon>
          <location length="21" offset="1000"/>
          <text>CO2 partial pressures</text>
        </annotation>
        <annotation id="T19">

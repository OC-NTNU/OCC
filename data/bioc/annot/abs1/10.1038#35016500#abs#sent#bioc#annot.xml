      <infon key="type">body</infon>
      <sentence>
        <infon key="n">2</infon>
        <offset>45</offset>
        <text>Global production of farmed fish and shellfish has more than doubled in the past 15 years.
</text>
        <annotation id="T1">
          <infon key="brat-id">T1</infon>
          <infon key="type">Increase</infon>
          <infon key="brat-type">text-bound</infon>

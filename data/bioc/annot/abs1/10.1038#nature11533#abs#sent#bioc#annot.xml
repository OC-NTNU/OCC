        </annotation>
        <annotation id="T13">
          <infon key="brat-id">T13</infon>
          <infon key="type">Variable</infon>
          <infon key="brat-type">text-bound</infon>
          <location length="57" offset="683"/>
          <text>the dense, below-ground biomass of bank-stabilizing roots</text>
        </annotation>
        <annotation id="T14">
          <infon key="brat-id">T14</infon>
          <infon key="type">Increase</infon>
          <infon key="brat-type">text-bound</infon>

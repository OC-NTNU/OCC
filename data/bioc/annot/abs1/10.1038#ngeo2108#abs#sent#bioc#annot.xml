 @article{Lenton_2014, title={Co-evolution of eukaryotes and ocean oxygenation in the Neoproterozoic era}, volume={7}, ISSN={1752-0908}, url={http://dx.doi.org/10.1038/ngeo2108}, DOI={10.1038/ngeo2108}, number={4}, journal={Nature Geoscience}, publisher={Nature Publishing Group}, author={Lenton, Timothy M. and Boyle, Richard A. and Poulton, Simon W. and Shields-Zhou, Graham A. and Butterfield, Nicholas J.}, year={2014}, month={Mar}, pages={257â€“265}}

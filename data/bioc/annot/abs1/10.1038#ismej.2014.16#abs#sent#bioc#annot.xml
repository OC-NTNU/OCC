 @article{McKie_Krisberg_2014, title={Phagotrophy by the picoeukaryotic green alga Micromonas: implications for Arctic Oceans}, ISSN={1751-7370}, url={http://dx.doi.org/10.1038/ismej.2014.16}, DOI={10.1038/ismej.2014.16}, journal={ISME J}, publisher={Nature Publishing Group}, author={McKie-Krisberg, Zaid M and Sanders, Robert W}, year={2014}, month={Feb}}

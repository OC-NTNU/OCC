        </annotation>
        <annotation id="T3">
          <infon key="brat-id">T3</infon>
          <infon key="type">Cause</infon>
          <infon key="brat-type">text-bound</infon>
          <location length="9" offset="94"/>
          <text>result of</text>
        </annotation>
        <annotation id="T12">
          <infon key="brat-id">T12</infon>
          <infon key="type">Variable</infon>
          <infon key="brat-type">text-bound</infon>
          <location length="32" offset="117"/>
          <text>atmospheric carbon dioxide (CO2)</text>
        </annotation>
        <relation id="E1">

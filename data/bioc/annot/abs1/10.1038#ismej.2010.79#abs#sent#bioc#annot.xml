        </annotation>
        <annotation id="T8">
          <infon key="brat-id">T8</infon>
          <infon key="type">Cause</infon>
          <infon key="brat-type">text-bound</infon>
          <location length="14" offset="676"/>
          <text>consequence of</text>
        </annotation>
        <annotation id="T5">
          <infon key="brat-id">T5</infon>
          <infon key="type">Change</infon>
          <infon key="brat-type">text-bound</infon>
          <location length="6" offset="664"/>
          <text>change</text>
        </annotation>
        <annotation id="T17">
          <infon key="brat-id">T17</infon>
          <infon key="type">Variable</infon>
          <infon key="brat-type">text-bound</infon>
          <location length="14" offset="731"/>
          <text>carbon dioxide</text>
        </annotation>
        <relation id="E7">
          <infon key="brat-id">E7</infon>
          <infon key="type">Increase</infon>
          <infon key="brat-type">event</infon>

        <infon key="n">1</infon>
        <offset>0</offset>
        <text>Will ocean acidification affect marine microbes?
</text>
        <annotation id="T11">
          <infon key="brat-id">T11</infon>
          <infon key="type">Variable</infon>
          <infon key="brat-type">text-bound</infon>
          <location length="27" offset="49"/>
          <text>The pH of the surface ocean</text>
        </annotation>
      </sentence>

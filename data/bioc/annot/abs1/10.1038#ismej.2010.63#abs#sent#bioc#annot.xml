        </annotation>
        <annotation id="T10">
          <infon key="type">Variable</infon>
          <infon key="brat-type">text-bound</infon>
          <infon key="brat-id">T10</infon>
          <location length="29" offset="855"/>
          <text>mineralization of naphthalene</text>
        </annotation>
        <annotation id="T11">
          <infon key="type">Variable</infon>
          <infon key="brat-type">text-bound</infon>
          <infon key="brat-id">T11</infon>
          <location length="19" offset="962"/>

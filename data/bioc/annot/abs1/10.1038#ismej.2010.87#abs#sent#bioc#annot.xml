 @article{Giebel_2010, title={Distribution of Roseobacter RCA and SAR11 lineages in the North Sea and characteristics of an abundant RCA isolate}, volume={5}, ISSN={1751-7370}, url={http://dx.doi.org/10.1038/ismej.2010.87}, DOI={10.1038/ismej.2010.87}, number={1}, journal={ISME J}, publisher={Nature Publishing Group}, author={Giebel, Helge-Ansgar and Kalhoefer, Daniela and Lemke, Andreas and Thole, Sebastian and Gahl-Janssen, Renate and Simon, Meinhard and Brinkhoff, Thorsten}, year={2010}, month={Jul}, pages={8â€“19}}

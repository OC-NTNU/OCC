      <sentence>
        <infon key="n">6</infon>
        <offset>745</offset>
        <text>Higher seawater temperatures at that time increased bacterial activity and organic matter regeneration.
</text>
        <annotation id="T14">
          <infon key="brat-id">T14</infon>
          <infon key="type">Variable</infon>
          <infon key="brat-type">text-bound</infon>
          <location length="34" offset="752"/>

          <text>silica dissolution</text>
        </annotation>
        <annotation id="T23">
          <infon key="type">Variable</infon>
          <infon key="brat-type">text-bound</infon>
          <infon key="brat-id">T23</infon>
          <location length="38" offset="1118"/>
          <text>natural assemblages of marine bacteria</text>
        </annotation>
        <relation id="E6">
          <infon key="type">Increase</infon>
          <infon key="brat-type">event</infon>
          <infon key="brat-id">E6</infon>
          <node refid="T6" role="Increase"/>

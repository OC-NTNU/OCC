          <text>dissolution</text>
        </annotation>
        <annotation id="T19">
          <infon key="type">Variable</infon>
          <infon key="brat-type">text-bound</infon>
          <infon key="brat-id">T19</infon>
          <location length="13" offset="27"/>
          <text>diatom silica</text>
        </annotation>
        <relation id="E1">
          <infon key="type">Decrease</infon>

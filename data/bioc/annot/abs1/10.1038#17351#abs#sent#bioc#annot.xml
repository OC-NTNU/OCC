          <node refid="T5" role="Theme"/>
        </relation>
      </sentence>
      <sentence>
        <infon key="n">7</infon>
        <offset>1276</offset>
        <text>Silica dissolution accompanied, and was caused by, bacterial colonization and hydrolytic attack.
</text>
        <annotation id="T7">
          <infon key="type">Cause</infon>
          <infon key="brat-type">text-bound</infon>
          <infon key="brat-id">T7</infon>
          <location length="9" offset="1316"/>
          <text>caused by</text>
        </annotation>
        <annotation id="T15">
          <infon key="type">Variable</infon>
          <infon key="brat-type">text-bound</infon>
          <infon key="brat-id">T15</infon>
          <location length="22" offset="1327"/>

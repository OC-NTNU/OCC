          <text>increased</text>
        </annotation>
        <annotation id="T15">
          <infon key="type">Variable</infon>
          <infon key="brat-type">text-bound</infon>
          <infon key="brat-id">T15</infon>
          <location length="11" offset="399"/>
          <text>iron supply</text>
        </annotation>
        <relation id="E4">

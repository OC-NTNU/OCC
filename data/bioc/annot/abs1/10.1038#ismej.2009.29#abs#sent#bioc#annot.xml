          <text>decreasing</text>
        </annotation>
        <annotation id="T22">
          <infon key="type">Variable</infon>
          <infon key="brat-type">text-bound</infon>
          <infon key="brat-id">T22</infon>
          <location length="34" offset="923"/>
          <text>disturbance by hydrodynamic forces</text>
        </annotation>
        <annotation id="T23">
          <infon key="type">Variable</infon>

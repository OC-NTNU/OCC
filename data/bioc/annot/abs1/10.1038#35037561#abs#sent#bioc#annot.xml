          <text>additions</text>
        </annotation>
        <annotation id="T29">
          <infon key="brat-id">T29</infon>
          <infon key="type">Variable</infon>
          <infon key="brat-type">text-bound</infon>
          <location length="26" offset="1479"/>
          <text>iron to the Southern Ocean</text>
        </annotation>

        </annotation>
        <annotation id="T18">
          <infon key="brat-id">T18</infon>
          <infon key="type">Variable</infon>
          <infon key="brat-type">text-bound</infon>
          <location length="16" offset="715"/>
          <text>atmospheric CO 2</text>
        </annotation>
        <annotation id="T19">
          <infon key="brat-id">T19</infon>
          <infon key="type">Variable</infon>
          <infon key="brat-type">text-bound</infon>

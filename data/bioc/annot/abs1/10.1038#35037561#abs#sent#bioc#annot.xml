          <infon key="brat-type">text-bound</infon>
          <location length="5" offset="409"/>
          <text>lower</text>
        </annotation>
        <annotation id="T13">
          <infon key="brat-id">T13</infon>
          <infon key="type">Variable</infon>
          <infon key="brat-type">text-bound</infon>
          <location length="34" offset="415"/>
          <text>concentrations of atmospheric CO 2</text>
        </annotation>
        <relation id="E4">

 @article{Kalvelage_2013, title={Nitrogen cycling driven by organic matter export in the South Pacific oxygen minimum zone}, volume={6}, ISSN={1752-0908}, url={http://dx.doi.org/10.1038/ngeo1739}, DOI={10.1038/ngeo1739}, number={3}, journal={Nature Geoscience}, publisher={Nature Publishing Group}, author={Kalvelage, Tim and Lavik, Gaute and Lam, Phyllis and Contreras, Sergio and Arteaga, Lionel and Löscher, Carolin R. and Oschlies, Andreas and Paulmier, Aurélien and Stramma, Lothar and Kuypers, Marcel M. M.}, year={2013}, month={Feb}, pages={228–234}}
</infon>

        </annotation>
        <annotation id="T4">
          <infon key="brat-id">T4</infon>
          <infon key="type">And</infon>
          <infon key="brat-type">text-bound</infon>
          <location length="3" offset="621"/>
          <text>and</text>
        </annotation>
        <annotation id="T19">
          <infon key="brat-id">T19</infon>
          <infon key="type">Variable</infon>
          <infon key="brat-type">text-bound</infon>
          <location length="19" offset="601"/>
          <text>silicic acid supply</text>
